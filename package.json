{
  "name": "recept",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@10.26.0",
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "test:api": "PGPASSWORD=${TEST_DB_PASSWORD:-recept} psql -h ${TEST_DB_HOST:-localhost} -p ${TEST_DB_PORT:-5432} -U ${TEST_DB_USER:-recept} -d ${TEST_DB_NAME:-recept} -f tests/api/seed-admin.sql && vitest run --config tests/api/vitest.config.ts",
    "lint": "pnpm -r lint",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "inbucket": "docker compose up -d inbucket",
    "inbucket:stop": "docker compose stop inbucket"
  },
  "devDependencies": {
    "vitest": "^4.0.15",
    "jose": "^6.0.10"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.2.7",
      "@types/react-dom": "19.2.3"
    }
  }
}
